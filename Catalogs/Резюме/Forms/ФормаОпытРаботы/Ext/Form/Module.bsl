//поместить введенные данные в таб часть документа перед закрытием формы
&НаКлиенте
Процедура Сохранить(Команда)
	СтруктураПараметров = Новый Структура();
	СтруктураПараметров.Вставить("НачалоДеятельности",НачалоДеятельности);
	СтруктураПараметров.Вставить("ОкончаниеДеятельности",ОкончаниеДеятельности);
	СтруктураПараметров.Вставить("Организация",Организация);
	СтруктураПараметров.Вставить("Должность",Должность);
	СтруктураПараметров.Вставить("Обязанности",Обязанности);
	
	ЭтаФорма.Закрыть(СтруктураПараметров);
КонецПроцедуры

&НаКлиенте
Процедура ЗакрытьБезСохранения(Команда)
	ЭтаФорма.Закрыть();
КонецПроцедуры

#КонецЕсли

