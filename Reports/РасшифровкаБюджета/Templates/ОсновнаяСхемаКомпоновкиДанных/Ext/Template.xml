			<presentationExpression>Формат(ПериодМесяц,"ДФ=ММММ")</presentationExpression>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	БДР.СтатьяДоходовРасходов КАК Статья,
	БДР.СуммаПлан КАК План,
	0 КАК Факт,
	НАЧАЛОПЕРИОДА(БДР.Период, МЕСЯЦ) КАК ПериодМесяц,
	БДР.Регистратор КАК Регистратор,
	ПРЕДСТАВЛЕНИЕССЫЛКИ(БДР.Регистратор) КАК РегистраторПредставление
ИЗ
	РегистрНакопления.БДР КАК БДР
ГДЕ
	БДР.Регистратор = &amp;Бюджет
	И БДР.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И БДР.СтатьяДоходовРасходов В ИЕРАРХИИ(&amp;СтатьяДР)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	БДРОбороты.СтатьяДоходовРасходов,
	0,
	БДРОбороты.СуммаФактОборот,
	НАЧАЛОПЕРИОДА(БДРОбороты.ПериодСекунда, МЕСЯЦ),
	БДРОбороты.Регистратор,
	ПРЕДСТАВЛЕНИЕССЫЛКИ(БДРОбороты.Регистратор)
ИЗ
	РегистрНакопления.БДР.Обороты(&amp;НачалоПериода, &amp;КонецПериода, Авто, СтатьяДоходовРасходов В ИЕРАРХИИ (&amp;СтатьяДР)) КАК БДРОбороты
	
ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	БДРОбороты.СтатьяДоходовРасходов,
	0,
	БДРОбороты.СуммаФактПрогнозОборот,
	НАЧАЛОПЕРИОДА(БДРОбороты.ПериодСекунда, МЕСЯЦ),
	БДРОбороты.Регистратор,
	ПРЕДСТАВЛЕНИЕССЫЛКИ(БДРОбороты.Регистратор)
ИЗ
	РегистрНакопления.БДР.Обороты(&amp;НачалоПериода, &amp;КонецПериода, Авто, СтатьяДоходовРасходов В ИЕРАРХИИ (&amp;СтатьяДР)) КАК БДРОбороты</query>
